<!DOCTYPE html>
<html>
  <head>
    <title>WhoKnowsMe</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= button_to "Logout", logout_user_path if logged_in? %>
    <%= yield %>
  </body>
</html>
